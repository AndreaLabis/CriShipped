{{#if page_object.length}}
<div class="container">
  <nav>
    <div class="custom-pagination">
      <ul class="pagination">
        {{#if paginate.hasPreviousPages}}
        {{!-- Incorrect syntax --}}
        {{!-- <li><a href="{{paginate.href(true)}}">&laquo;</a></li> --}}
        {{else}}
        <li class="disabled"><span>&laquo;</span></li>
        {{/if}}
        {{#each page as |pages|}}
        {{!-- {{#each pages as |page|}} --}}
        {{!-- {{#for page in pages}} --}}
        {{#if page.number}}
        {{#if (paginate.page page.number)true}}
        <li class="active"><span>{{page.number}} <span class="sr-only">(current)</span></span></li>
        {{else}}
        <li><a href="?page={{page.number}}">{{page.number}}</a></li>
        {{/if}}
        {{else}}
        <li class="disabled"><a href="#">&hellip;</a></li>
        {{/if}}
        {{/each}}
        {{!-- {{/for}} --}}
        {{!-- {{#if paginate.hasNextPages(pageCount)}} --}}
        {{!-- <li><a href="{{paginate.href()}}">&raquo;</a></li> --}}
        {{else}}
        <li class="disabled"><span>&raquo;</span></li>
        {{/if}}
      </ul>
    </div>
  </nav>
</div>
{{!-- {{/if}} --}}