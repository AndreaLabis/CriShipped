<h5 class="brand_heading">Brands</h5> 
{{!-- {{#each brands as |b|}} --}}
{{#each b as |brands|}}
{{!-- {{#for b in brands}}            --}}
<div class="brand_list-wrapper">
  {{#if (b.slug selected_brand)true}}
  <a href="/catalogue/{{selected_category}}/{{b.slug}}/" class="brand_selected">{{b.name}}</a>
  {{else}}
  <a href="/catalogue/{{selected_category}}/{{b.slug}}/" class="brand_not-selected">{{b.name}}</a>
  {{/if }}
</div>
{{/each}}
{{!-- {{/for}} --}}

